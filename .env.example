# TikTok Product Scout Environment Variables
# Copy this file to .env and fill in your actual values

# Database
DATABASE_URL=sqlite:///data/db/products.db

# Proxy configuration (optional, comma-separated)
# PROXY_LIST=http://user:pass@proxy1:8080,http://user:pass@proxy2:8080
PROXY_LIST=

# Third-party API keys
# TikTok Shop data providers
KALODATA_API_KEY=
FASTMOSS_API_KEY=

# Alerting - Discord
DISCORD_WEBHOOK_URL=

# Alerting - Email (if using)
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=
SMTP_PASSWORD=
EMAIL_RECIPIENTS=

# Alerting - Generic webhook
WEBHOOK_URL=

# Logging
LOG_LEVEL=INFO
LOG_FORMAT=json

# API Configuration
API_HOST=0.0.0.0
API_PORT=8000

# Security (for production)
SECRET_KEY=
API_KEY=
